<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data>
		<record id="medicine_info_tree_view" model="ir.ui.view">
			<field name="name">medicine.info.tree</field>
			<field name="model">medicine.info</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
					<field name="reference_number"/>
					<field name="is_major"/>
					<field name="company_name"/>
					<field name="dosage_form"/>
				</tree>
			</field>
		</record>
		

		<record id="action_medicine_info" model="ir.actions.act_window">
		   <field name="name">Medicine Info</field>
		   <field name="res_model">medicine.info</field>
		   <field name="view_mode">tree</field>
		</record>


		<menuitem id="medical_store" name="Medical store" sequence="1"/>
		<menuitem id="medicine_info" name="Medicine" sequence="2" action="action_medicine_info" parent="medical_store"/>

	</data>
</odoo>